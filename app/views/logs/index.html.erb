<h1>Listing logs</h1>

<table>
  <tr>
    <th>Time</th>
    <th>Status</th>
    <th>Executiontime</th>
    <th>Memorysize</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @logs.each do |log| %>
  <tr>
    <td><%= log.time %></td>
    <td><%= log.status %></td>
    <td><%= log.executionTime %></td>
    <td><%= log.memorySize %></td>
    <td><%= link_to 'Show', log %></td>
    <td><%= link_to 'Edit', edit_log_path(log) %></td>
    <td><%= link_to 'Destroy', log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Log', new_log_path %>
